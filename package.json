  "scripts": {
    "botfather:serve": "nx serve botfather",
    "bot-client:build:watch": "nodemon --config ./nodemon/bot-client-nodemon-config.json",
    "accounts-reg:build:watch": "nodemon --config ./nodemon/accounts-reg-nodemon-config.json",
    "frontend:dev": "nx serve frontend",
    "codegen": "graphql-codegen --config codegen.ts",
    "compileTailwind": "npx tailwindcss -i ./apps/frontend/pages/styles.css -o ./apps/frontend/styles/tailwind-output.css --watch",
    "botclient:run:watch": "nodemon --config ./nodemon/watch-test-client-nodemon-config.json client-dist.test.ts",
    "botfather:test:watch": "nx test botfather --watch --verbose",
    "botfather:e2e": "nx e2e botfather-e2e",
    "botfather:e2e:watch": "nx e2e botfather-e2e --watch",
    "botfather:e2e:coverage": "nx e2e botfather-e2e --coverage",
    "bot-client:build": "nx build bot-client",
    "bot-client:test": "nx test bot-client --coverage",
    "bot-client:e2e:watch": "nx e2e bot-client-e2e --coverage --watch --verbose",
    "bot-client:test:watch": "nx test bot-client --watch --verbose",
    "graphql:init": "graphql-codegen init",
    "complie-typeorm-config": "tsc typeorm/typeorm.config.ts --skipLibCheck --esModuleInterop --emitDecoratorMetadata --experimentalDecorators --outDir .",
    "typeorm-create-migration": "npm run complie-typeorm-config && typeorm migration:create typeorm/initial",
    "typeorm-generate-migration": "npm run complie-typeorm-config && typeorm migration:generate ./typeorm -d typeorm/typeorm.config.js"
  },
